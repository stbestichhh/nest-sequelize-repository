  "files": [
    "index.js",
    "index.d.ts",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@nestjs/common": "^11.1.11",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.19.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.7.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sqlite3": "^5.1.7",
    "ts-jest": "^29.4.6",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@nestjs/common": "11.x",
    "reflect-metadata": "0.x",
    "rxjs": "7.x",
    "sequelize": "6.x",
    "sequelize-typescript": "2.x"
  }
